
pipeline{
    agent any
        stages{
            
            stage("GIT"){
                steps{
                    git "https://github.com/QAC-KHAN/java3.git"
                }
            }
            stage("Maven"){
                steps{
                    bat "mvn build"
                }
            }
              stage("Message"){
                steps{
                    echo "3rd Step ending"
                }
            }
        }
}
    
